{
  "name": "@cdw0424/super-prompt",
  "version": "3.1.40",
  "description": "Production-ready prompt engineering toolkit with modular architecture, AMR, SDD workflow, and dual IDE support",
  "main": "index.js",
  "bin": {
    "super-prompt": "./bin/super-prompt",
    "codex-amr": "./bin/codex-amr.js",
    "sp-init": "./bin/sp-init",
    "sp-reinit": "./bin/sp-reinit",
    "sp-reinit-sp": "./bin/sp-reinit-sp"
  },
  "scripts": {
    "prepack": "find . -type d -name __pycache__ -prune -exec rm -rf {} + >/dev/null 2>&1 || true; find . -name '*.pyc' -delete >/dev/null 2>&1 || true",
    "postinstall": "node install.js",
    "test": "node --test",
    "dev": "./bin/super-prompt",
    "dev:init": "./bin/super-prompt super:init",
    "codex:plan": "bin/codex-high",
    "codex:exec": "bin/codex-medium",
    "amr:rules": "super-prompt amr:rules || npx codex-amr init --target .",
    "amr:print": "node ./bin/codex-amr.js print-bootstrap",
    "amr:qa": "scripts/codex/prompt-qa.sh"
  },
  "keywords": [
    "ai",
    "development",
    "persona",
    "cursor",
    "cli",
    "prompt-engineering",
    "productivity",
    "automation",
    "amr",
    "model-routing",
    "codex"
  ],
  "author": "Daniel Choi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdw0424/super-prompt.git"
  },
  "bugs": {
    "url": "https://github.com/cdw0424/super-promt/issues"
  },
  "homepage": "https://github.com/cdw0424/super-promt#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "files": [
    "bin",
    "src",
    ".super-prompt",
    "install.js",
    "README.md",
    "CHANGELOG.md",
    ".cursor"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "ajv": "^8.17.1",
    "better-sqlite3": "^12.2.0",
    "ioredis": "^5.7.0",
    "zod": "^4.1.8"
  }
}
