{
  "name": "@cdw0424/super-prompt",
  "version": "3.1.73",
  "description": "Super Prompt v3 (Enhanced)",
  "main": ".super-prompt/cli.py",
  "bin": {
    "super-prompt": "bin/super-prompt",
    "codex-amr": "bin/codex-amr.js",
    "sp": "bin/sp",
    "sp-setup-shell": "bin/sp-setup-shell",
    "sp-init": "bin/sp-init",
    "sp-reinit": "bin/sp-reinit",
    "sp-reinit-sp": "bin/sp-reinit-sp"
  },
  "scripts": {
    "prepack": "find . -type d -name __pycache__ -prune -exec rm -rf {} + >/dev/null 2>&1 || true; find . -name '*.pyc' -delete >/dev/null 2>&1 || true",
    "preinstall": "mkdir -p ${NPM_CONFIG_CACHE:-/tmp/.npm-cache} || true",
    "postinstall": "node install.js",
    "test": "node --test",
    "dev": "./bin/super-prompt",
    "dev:init": "./bin/super-prompt super:init",
    "codex:init": "super-prompt codex-init || node ./bin/codex-amr.js init --target .codex",
    "codex:plan": "bin/codex-high",
    "codex:exec": "bin/codex-medium",
    "amr:rules": "super-prompt amr:rules || npx codex-amr init --target .",
    "amr:print": "node ./bin/codex-amr.js print-bootstrap",
    "amr:qa": "scripts/codex/prompt-qa.sh"
  },
  "keywords": [
    "ai",
    "development",
    "persona",
    "cursor",
    "cli",
    "prompt-engineering",
    "productivity",
    "automation",
    "amr",
    "model-routing",
    "codex"
  ],
  "author": "Daniel Choi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdw0424/super-prompt.git"
  },
  "bugs": {
    "url": "https://github.com/cdw0424/super-prompt/issues"
  },
  "homepage": "https://github.com/cdw0424/super-prompt#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "files": [
    "bin",
    "src",
    "packages",
    "scripts",
    ".super-prompt",
    "install.js",
    "README.md",
    "CHANGELOG.md",
    ".cursor"
  ],
  "publishConfig": {
    "access": "public"
  },
  "pack": {
    "destination": "builds/"
  },
  "devDependencies": {
    "ajv": "^8.17.1",
    "better-sqlite3": "^12.2.0",
    "ioredis": "^5.7.0",
    "zod": "^4.1.8"
  }
}
