{
  "name": "@cdw0424/super-prompt",
  "version": "4.4.2",
  "description": "Super Prompt v4.4.2 (Streamlined Architecture - Pure stdio Mode)",
  "bin": {
    "super-prompt": "bin/super-prompt",
    "sp-mcp": "bin/sp-mcp"
  },
  "scripts": {
    "clean:py": "find . -type d -name __pycache__ -prune -exec rm -rf {} + >/dev/null 2>&1 || true; find . -name '*.pyc' -delete >/dev/null 2>&1 || true; find packages -type d -name __pycache__ -prune -exec rm -rf {} + >/dev/null 2>&1 || true; find packages -name '*.pyc' -delete >/dev/null 2>&1 || true",
    "clean": "rm -rf dist || true && npm run clean:py",
    "build": "echo 'Build complete - JavaScript project'",
    "prepack": "npm run clean && node scripts/prepare-python-dist.js && npm run build",
    "preinstall": "mkdir -p ${NPM_CONFIG_CACHE:-/tmp/.npm-cache} || true",
    "postinstall": "node install.js",
    "test": "node --test",
    "dev": "./bin/super-prompt",
    "dev:init": "./bin/super-prompt super:init",
    "amr:rules": "super-prompt super:init",
    "amr:print": "cat .cursor/rules/*-sdd-*.mdc",
    "amr:qa": "scripts/codex/prompt-qa.sh",
    "sp:init": "super-prompt super:init",
    "sp:rules": "super-prompt super:init && cat .cursor/rules/*-sdd-*.mdc || true",
    "sp:mcp": "sp-mcp",
    "sp:doctor": "super-prompt doctor"
  },
  "keywords": [
    "ai",
    "development",
    "persona",
    "cursor",
    "cli",
    "prompt-engineering",
    "productivity",
    "automation",
    "amr",
    "model-routing",
    "codex"
  ],
  "author": "Daniel Choi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdw0424/super-prompt.git"
  },
  "bugs": {
    "url": "https://github.com/cdw0424/super-prompt/issues"
  },
  "homepage": "https://github.com/cdw0424/super-prompt#readme",
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "files": [
    "bin",
    "src/",
    "dist/",
    "templates/",
    "packages/core-py/",
    "packages/cursor-assets/",
    "packages/codex-assets/",
    "install.js",
    "README.md",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18.17"
  },
  "publishConfig": {
    "access": "public"
  },
  "pack": {
    "destination": "builds/"
  },
  "devDependencies": {
    "ajv": "^8.17.1",
    "better-sqlite3": "^12.2.0",
    "ioredis": "^5.7.0",
    "zod": "^4.1.8"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.0"
  }
}
