{
  "name": "@cdw0424/super-prompt",
  "version": "4.0.22",
  "description": "Super Prompt v4.0.0 (MCP Architecture)",
  "bin": {
    "super-prompt": "bin/super-prompt"
  },
  "scripts": {
    "prepack": "find . -type d -name __pycache__ -prune -exec rm -rf {} + >/dev/null 2>&1 || true; find . -name '*.pyc' -delete >/dev/null 2>&1 || true",
    "preinstall": "mkdir -p ${NPM_CONFIG_CACHE:-/tmp/.npm-cache} || true",
    "postinstall": "node install.js",
    "test": "node --test",
    "dev": "./bin/super-prompt",
    "dev:init": "./bin/super-prompt init",
    "amr:rules": "super-prompt init",
    "amr:print": "cat .cursor/rules/*-sdd-*.mdc",
    "amr:qa": "scripts/codex/prompt-qa.sh"
  },
  "keywords": [
    "ai",
    "development",
    "persona",
    "cursor",
    "cli",
    "prompt-engineering",
    "productivity",
    "automation",
    "amr",
    "model-routing",
    "codex"
  ],
  "author": "Daniel Choi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdw0424/super-prompt.git"
  },
  "bugs": {
    "url": "https://github.com/cdw0424/super-prompt/issues"
  },
  "homepage": "https://github.com/cdw0424/super-prompt#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "files": [
    "bin",
    "packages/cli-node/",
    "packages/codex-assets/",
    "packages/core-py/dist/",
    "packages/core-py/pyproject.toml",
    "packages/core-py/pytest.ini",
    "packages/core-py/super_prompt/",
    "packages/core-py/tests/",
    "install.js",
    "README.md",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "pack": {
    "destination": "builds/"
  },
  "devDependencies": {
    "ajv": "^8.17.1",
    "better-sqlite3": "^12.2.0",
    "ioredis": "^5.7.0",
    "zod": "^4.1.8"
  }
}
